{
    "start_url": "https://webally.co.za",
    "max_depth": 10,
    "delay": 0,
    "user_agent": "WebAtlas Crawler (https://github.com/charlpcronje/WebAtlas)",
    "request_timeout": 30,
    "max_retries": 3,
    "output_dir": "./output/webally",
    "exclude_patterns": [
      "/tag/",
      "/category/",
      "/author/",
      "/page/[2-9]",
      "\\.pdf$",
      "\\.jpg$",
      "\\.png$"
    ],
    "respect_robots_txt": true,
    "crawl_subdomains": true,
    "max_pages": 100,
    "follow_redirects": true,
    "headers": {
      "Accept-Language": "en-US,en;q=0.9"
    },
    "data_to_extract": [
      "title",
      "meta_description",
      "h1",
      "links",
      "images"
    ],
    "save_html": true,
    "create_sitemap": true,
    "log_level": "INFO"
  }